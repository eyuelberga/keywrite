{
    "name": "@keywrite/web",
    "version": "1.0.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "directory": "packages/web",
        "url": "https://github.com/eyuelberga/keywrite.git"
    },
    "scripts": {
        "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
        "prepack": "yarn build",
