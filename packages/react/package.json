{
    "name": "@keywrite/react",
    "version": "0.1.2",
    "private": true,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "directory": "packages/react",
        "url": "https://github.com/eyuelberga/keywrite.git"
    },
    "scripts": {
        "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
        "prepack": "yarn build",
        "build": "yarn clean && tsc --build && cp \"./package.json\" ./dist/",
        "test": "jest --passWithNoTests",
        "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=10",
        "prettier-write": "prettier --write \"{src,test}/**/*.{js,ts,jsx,tsx,json,css,scss,html}\""
    },
    "dependencies": {
        "@keywrite/web": "^1.0.0"
    },
    "devDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
