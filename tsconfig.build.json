{
    "compilerOptions": {
        "target": "es5",
        "outDir": "./dist",
        "module": "commonjs",
        "moduleResolution": "node",
        "jsx": "react",
        "strict": true,
        "declaration": true,
        "declarationMap": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
